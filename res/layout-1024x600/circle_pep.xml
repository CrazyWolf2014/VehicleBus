<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:clickable="true" android:layout_width="-1" android:layout_height="-1">
    <include android:id="@id/top" android:layout_width="-1" android:layout_height="-2" android:layout_below="@id/main_head" layout="@layout/circle_head" />
    <ImageView android:id="@id/tata_image" android:layout_width="-2" android:layout_height="-2" android:layout_marginTop="2dp" android:src="@drawable/girl" android:layout_below="@id/top" android:layout_centerHorizontal="true" />
    <ScrollView android:scrollbars="0" android:layout_width="-1" android:layout_height="-1" android:layout_below="@id/tata_image">
        <RelativeLayout android:paddingLeft="20dp" android:paddingRight="20dp" android:layout_width="-1" android:layout_height="-2"
            <RelativeLayout android:id="@id/address" android:layout_width="-1" android:layout_height="50dp">
                <TextView android:textColor="@color/white" android:id="@id/address" android:layout_width="80dp" android:layout_height="-2" android:text="@string/address_text" android:layout_centerVertical="true" />
                <TextView android:textColor="@color/white" android:id="@id/address_text" android:layout_width="-2" android:layout_height="-2" android:layout_toRightOf="@id/address" android:layout_centerVertical="true" />
            </RelativeLayout>
            <ImageView android:id="@id/fengexian8" android:background="@drawable/fengexian" android:layout_width="-1" android:layout_height="-2" android:layout_below="@id/address" />
            <RelativeLayout android:id="@id/sex" android:layout_width="-1" android:layout_height="50dp" android:layout_below="@id/fengexian8">
                <TextView android:textColor="@color/white" android:id="@id/sex" android:layout_width="80dp" android:layout_height="-2" android:text="@string/sex_text" android:layout_centerVertical="true" />
                <TextView android:textColor="@color/white" android:id="@id/sex_text" android:layout_width="-2" android:layout_height="-2" android:layout_toRightOf="@id/sex" android:layout_centerVertical="true" />
            </RelativeLayout>
            <ImageView android:id="@id/fengexian9" android:background="@drawable/fengexian" android:layout_width="-1" android:layout_height="-2" android:layout_below="@id/sex" />
            <RelativeLayout android:id="@id/birthday" android:layout_width="-1" android:layout_height="50dp" android:layout_below="@id/fengexian9">
                <TextView android:textColor="@color/white" android:id="@id/birthdays" android:layout_width="80dp" android:layout_height="-2" android:text="@string/data_birth" android:layout_centerVertical="true" />
                <TextView android:textColor="@color/white" android:id="@id/birthday_text" android:layout_width="-2" android:layout_height="-2" android:layout_toRightOf="@id/birthdays" android:layout_centerVertical="true" />
            </RelativeLayout>
            <ImageView android:id="@id/fengexian10" android:background="@drawable/fengexian" android:layout_width="-1" android:layout_height="-2" android:layout_below="@id/birthday" />
            <RelativeLayout android:id="@id/contact_way" android:layout_width="-1" android:layout_height="50dp" android:layout_below="@id/fengexian10">
                <TextView android:textColor="@color/white" android:id="@id/contact" android:layout_width="80dp" android:layout_height="-2" android:text="@string/data_phone" android:layout_centerVertical="true" />
                <TextView android:textColor="@color/white" android:id="@id/contact_text" android:layout_width="-2" android:layout_height="-2" android:layout_toRightOf="@id/contact" android:layout_centerVertical="true" />
            </RelativeLayout>
            <RelativeLayout android:id="@id/button" android:layout_width="-2" android:layout_height="-2" android:layout_marginBottom="20dp" android:layout_below="@id/contact_way" android:layout_centerHorizontal="true">
                <LinearLayout android:orientation="0" android:id="@id/title" android:layout_width="-2" android:layout_height="-2">
                    <ImageView android:id="@id/friend" android:layout_width="-1" android:layout_height="-2" android:src="@drawable/friend" android:layout_weight="1.0" />
                    <ImageView android:id="@id/private_chat" android:layout_width="-1" android:layout_height="-2" android:src="@drawable/private_chat" android:layout_weight="1.0" />
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
</RelativeLayout>
